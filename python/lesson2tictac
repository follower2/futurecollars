def print_board(board):
    print("\n" + board[0] + " | " + board[1] + " | " + board[2])
    print("---------")
    print(board[3] + " | " + board[4] + " | " + board[5])
    print("---------")
    print(board[6] + " | " + board[7] + " | " + board[8] + "\n")

if __name__ == "__main__":
    board = ['-', '-', '-',
             '-', '-', '-',
             '-', '-', '-']
    print_board(board)